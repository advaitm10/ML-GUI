<Window x:Class="MLGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MLGui"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl TabStripPlacement="Left">
            <TabItem>
                <TabItem.Header>
                    <TextBlock Text="Regression" FontSize="20"/>
                </TabItem.Header>
                <Grid>
                    <TextBlock Text="Choose Data" HorizontalAlignment="Center"/>
                    <TextBox x:Name="DataSelector" HorizontalAlignment="Center" Width="300" Height="20"/>
                </Grid>
            </TabItem>
            <TabItem>
                <TabItem.Header>
                    <TextBlock Text="Classification" FontSize="20"/>
                </TabItem.Header>
            </TabItem>
        </TabControl>
        <local:StartMenu Visibility="Collapsed"/>
    </Grid>
</Window>
